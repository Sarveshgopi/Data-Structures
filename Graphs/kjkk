std::unordered_map<char,int> a;
        int i,c=0,out=0;
        for(i=0;i<s.size();i++)
        {
            if(a[s[i]]!=0 || i==s.size()-1)
            {
                out=max(out,c);
                c=0;
                a.clear();
            }
            else
            {
                c++;
                a[s[i]]++;
            }
        }
